{
  "group": {
    "parent": "config",
    "prefix": "group",
    "body": [
      "<group id=\"${1:default}\">",
      "    ${2}",
      "</group>"
    ],
    "description": "Insert a cron group element"
  },
  "job": {
    "parent": "group",
    "prefix": "job",
    "body": [
      "<job name=\"${1}\" instance=\"${2}\" method=\"${3:execute}\">",
      "    <schedule>${4:* * * * *}</schedule>",
      "</job>"
    ],
    "description": "Insert a cron job element"
  }
} 